@if(TempData["info"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/Toastr/Toastr.min.js"></script>
    <script type="text/javascript">
        toastr.info('@TempData["info"]');
    </script>
}
